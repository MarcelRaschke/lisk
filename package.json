{
	"name": "lisk",
	"version": "1.5.0-alpha.0",
	"description": "Lisk blockchain application platform",
	"author":
		"Lisk Foundation <admin@lisk.io>, lightcurve GmbH <admin@lightcurve.io>",
	"license": "GPL-3.0",
	"private": true,
	"keywords": ["cryptocurrency", "blockchain", "lisk", "nodejs", "javascript"],
	"homepage": "https://github.com/LiskHQ/lisk#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LiskHQ/lisk.git"
	},
	"bugs": {
		"url": "https://github.com/LiskHQ/lisk/issues"
	},
	"engines": {
		"node": "10.14.1",
		"npm": "6.4.1"
	},
	"main": "app.js",
	"scripts": {
		"start": "node app.js",
		"console": "node scripts/console.js",
		"lint": "./node_modules/.bin/eslint .",
		"lint:fix": "./node_modules/.bin/eslint --fix .",
		"format":
			"prettier --write \"./*.{js,json,md}\" \"{api,config,db,docs,helpers,logic,modules,schema,scripts,tasks,test}{,/**}/*.{js,json,md}\"",
		"test": "./node_modules/.bin/grunt",
		"grunt": "./node_modules/.bin/grunt",
		"mocha": "./node_modules/.bin/mocha",
		"docs:build": "jsdoc -c docs/conf.json --verbose --pedantic",
		"docs:serve": "./node_modules/.bin/http-server docs/jsdoc/",
		"check:dependencies": "./node_modules/.bin/snyk protect",
		"prepublishOnly": "npm run check:dependencies"
	},
	"dependencies": {
		"async": "3.1.1",
		"bignumber.js": "9.0.0",
		"bluebird": "3.7.2",
		"body-parser": "1.19.0",
		"bytebuffer": "5.0.1",
		"change-case": "4.1.1",
		"colors": "1.4.0",
		"commander": "4.1.0",
		"compression": "1.7.4",
		"cors": "2.8.5",
		"debug": "4.1.1",
		"deep-diff": "1.0.2",
		"express": "4.17.1",
		"express-domain-middleware": "0.1.0",
		"express-query-int": "3.0.0",
		"express-rate-limit": "5.0.0",
		"fs-extra": "8.1.0",
		"ip": "1.1.5",
		"js-yaml": "3.13.1",
		"json-refs": "3.0.13",
		"lisk-commander": "3.0.1",
		"lisk-elements": "3.0.1",
		"lodash": "4.17.15",
		"lisk-newrelic": "LiskHQ/lisk-newrelic#d1ca762",
		"method-override": "3.0.0",
		"newrelic": "6.3.0",
		"pm2": "4.2.3",
		"pg-monitor": "1.3.1",
		"pg-promise": "10.4.0",
		"popsicle": "12.0.5",
		"randomstring": "1.1.5",
		"redis": "2.8.0",
		"rimraf": "3.0.1",
		"semver": "7.1.2",
		"socket.io": "2.3.0",
		"socketcluster": "15.0.5",
		"socketcluster-client": "15.1.0",
		"sodium-native": "2.4.9",
		"strftime": "0.10.0",
		"swagger-node-runner": "0.7.3",
		"sway": "2.0.6",
		"tempy": "0.3.0",
		"valid-url": "1.0.9",
		"wamp-socket-cluster": "2.0.0-beta.4",
		"z-schema": "4.2.2"
	},
	"devDependencies": {
		"browserify-bignum": "1.3.0",
		"chai": "4.2.0",
		"chai-as-promised": "7.1.1",
		"co-mocha": "1.2.2",
		"coveralls": "3.0.9",
		"eslint": "6.8.0",
		"eslint-config-airbnb-base": "14.0.0",
		"eslint-config-lisk-base": "1.2.2",
		"eslint-plugin-chai-expect": "2.1.0",
		"eslint-plugin-import": "2.20.0",
		"eslint-plugin-mocha": "6.2.2",
		"faker": "4.1.0",
		"find": "0.3.0",
		"grunt": "1.0.4",
		"grunt-exec": "3.0.0",
		"http-server": "0.12.1",
		"husky": "4.2.1",
		"istanbul": "0.4.5",
		"istanbul-middleware": "0.2.2",
		"jsdoc": "3.6.3",
		"jsdox": "0.4.10",
		"lint-staged": "10.0.7",
		"mocha": "7.0.1",
		"moment": "2.24.0",
		"node-mocks-http": "^1.7.3",
		"prettier": "1.19.1",
		"rewire": "4.0.1",
		"rx": "4.1.0",
		"sinon": "8.1.1",
		"sinon-chai": "3.4.0",
		"snyk": "^1.122.0",
		"stampit": "4.3.1",
		"supertest": "4.0.2"
	},
	"snyk": true,
	"lisk": {
		"minVersion": "1.1.0-rc.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push":
				"./node_modules/.bin/eslint $(git ls-files '*.js') && npm run check:dependencies"
		}
	}
}
